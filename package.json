{
  "name": "gov-jobs-curator",
  "version": "1.0.0",
  "description": "Government jobs aggregation and curation system",
  "main": "dist/index.js",
  "scripts": {
    "dev": "ts-node-dev --respawn src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "jest",
    "lint": "eslint src/**/*.ts",
    "scrape:ssc": "ts-node scripts/run-scraper.ts --source=SSC",
    "scrape:upsc": "ts-node scripts/run-scraper.ts --source=UPSC",
    "clean:expired": "ts-node scripts/clean-expired.ts"
  },
  "keywords": ["job-scraping", "government-jobs", "supabase", "typescript"],
  "author": "Anuj Kumar Mishra",
  "license": "MIT",
  "dependencies": {
    "@supabase/supabase-js": "^2.39.0",
    "axios": "^1.6.2",
    "cheerio": "^1.0.0-rc.12",
    "date-fns": "^3.0.6",
    "dotenv": "^16.3.1",
    "natural": "^6.10.2",
    "node-cron": "^3.0.3",
    "pdf-parse": "^1.1.1",
    "pino": "^8.17.2",
    "pino-pretty": "^10.3.1",
    "puppeteer": "^21.6.1",
    "sharp": "^0.33.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/node": "^20.10.5",
    "@types/node-cron": "^3.0.11",
    "@types/pdf-parse": "^1.1.4",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3"
  }
}
